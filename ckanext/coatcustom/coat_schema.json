{
  "scheming_version": 1,
  "dataset_type": "dataset",
  "about": "A reimplementation of the default CKAN dataset schema",
  "about_url": "http://github.com/ckan/ckanext-scheming",
  "dataset_fields": [
    {
      "field_name": "title",
      "label": "Title",
      "preset": "title",
      "form_placeholder": "eg. A descriptive title"
    },
    {
      "field_name": "name",
      "required": true,
      "label": "URL",
      "preset": "dataset_slug",
      "form_placeholder": "eg. my-dataset"
    },
    {
      "field_name": "version",
      "label": "Version",
      "validators": "ignore_missing unicode package_version_validator",
      "form_snippet": null
    },
    {
      "field_name": "owner_org",
      "required": true,
      "label": "Namespace",
      "preset": "dataset_organization"
    },
    {
      "field_name": "notes",
      "required": true,
      "label": "Description",
      "validators": "ignore_missing",
      "form_snippet": "markdown.html",
      "form_placeholder": "eg. Some useful notes about the data"
    },
    {
      "field_name": "tag_string",
      "required": true,
      "label": "tags (keywords)",
      "preset": "tag_string_autocomplete",
      "validators": "convert_to_tags(coat_vocabulary)",
      "form_placeholder": "choose one or more keywords from the COAT list",
      "help_text": "aerial imagery, air temperature, albedo, artificial nests, barcoding, birch forest, camera traps, dendroecology, distance sampling, dwarf shrub heath, experimental exclosures, flight intercept traps, forest, GPS tags, grassy tundra, ground ice, ground temperature, heath, humidity, hummock tundra, hunting records, ice thickness, icing, live trapping, meadow, metabarcoding, microsatellites, modelling, moss tundra, NDVI, NIRS, pellet counts, point intercept, population modelling, precipitation, radiation, rain on snow, remote sensing, repeat photography, satellite imagery, satellite tags, scat analysis, snap trapping, snow beds, snow depth, snow melt, snow modelling, snow pits, snow structure, snow tracking, soil temperature, stable isotopes, surveys, tall shrub, temperature, temperature logger, timing of icing, timing of snow melt, tundra, weather station, wind"
    },
